/* Dark Mode */


:root {
    --bg-color: white;
    --border-color: #999999;
    --btn-bg-color-hover:  #dddddd;
    --btn-bg-color: transparent;
    --btn-border: 1px solid #dddddd;
    --location-color: black;
    --day-color: #575757;
    --left-message-bg-color: #e5e4e9;
    --left-message-color: black;
    --left-svg-fill: #e5e4e9;
    --btn-text-color: black;
    --text-link-colors: black;
 }


 
[data-theme="dark"] {
    --bg-color: black;
    --border-color: #5e5e5e;
    --btn-bg-color-hover: #202020;
    --btn-border: 1px solid #202020;
    --location-color: white;
    --day-color: rgb(201, 201, 201);
    --left-message-bg-color: #26252a;
    --left-message-color: white;
    --left-svg-fill: #26252a;
    --btn-text-color: white;
    --text-link-colors: white;
}
  
  .theme-switch input {
    display:none;
  }
  
  
  
























/*  סלידר אין סופי  */


.wrapper {
  overflow: hidden;
}

.sliding-background {
  /* background: url("/docs/compare.png") repeat-x; */
  display: flex;
  align-items: flex-start;
  height: auto;
  margin-left: -1200px;
  margin-right: -20px;
  width: 5000px;
  animation: slide 50s linear infinite;
  margin-bottom: 90px;
}

@keyframes slide{
  0%{
    transform: translate3d(0, 0, 0);
  }
  100%{
    transform: translate3d(-3392px, 0, 0);
  }
}

.slide{
  width:277px;
  margin-right: 120px;
  display: inline-block;
  vertical-align: middle;
}






