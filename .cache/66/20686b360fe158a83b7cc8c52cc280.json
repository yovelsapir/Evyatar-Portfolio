dy {\r\n    margin: 0;\r\n    font-family: 'Varela', sans-serif;\r\n    font-weight: 100;\r\n    scroll-behavior: smooth;\r\n    overflow-x: hidden;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n    color: black;\r\n}\r\n\r\n.page {\r\n    margin-bottom: 200px;\r\n    margin-top: 50px;\r\n    width: 100%;\r\n    position: relative;\r\n    transition: 1s all;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.group {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-top: 150px;\r\n}\r\n\r\n.element {\r\n    position: relative;\r\n    transition: 0.5s all;\r\n}\r\n\r\n.hidden {\r\n    display: none;\r\n}\r\n\r\n.message-block {\r\n    position: relative;\r\n    transition: 0.5s all;\r\n    -webkit-transition: 0.5s all;\r\n    -moz-transition: 0.5s all;\r\n    -ms-transition: 0.5s all;\r\n    -o-transition: 0.5s all;\r\n}\r\n\r\n.message-block-last {\r\n    margin-bottom: 200px;\r\n}\r\n\r\n.right .message,\r\n.left .message {\r\n    position: relative;\r\n    transition: 1s opacity, all 0.4s;\r\n    -webkit-transition: 1s opacity, all 0.4s;\r\n    -moz-transition: 1s opacity, all 0.4s;\r\n    -ms-transition: 1s opacity, all 0.4s;\r\n    -o-transition: 1s opacity, all 0.4s;\r\n    opacity: 0;\r\n}\r\n\r\n@keyframes bubbleleft {\r\n    0% {\r\n        opacity: 0;\r\n        transform: rotate(-45deg) scaleX(-1);\r\n        left: -4.5px;\r\n    }\r\n\r\n    100% {\r\n        opacity: 1;\r\n        transform: rotate(0) scaleX(-1);\r\n    }\r\n}\r\n\r\n@keyframes bubbleRight {\r\n    0% {\r\n        opacity: 0;\r\n        transform: rotate(45deg) scaleX(1);\r\n        right: -4.5px;\r\n    }\r\n\r\n    100% {\r\n        opacity: 1;\r\n        transform: rotate(0) scaleX(1);\r\n    }\r\n}\r\n\r\n.left>svg {\r\n    position: absolute;\r\n    left: -16.5px;\r\n    bottom: -2px;\r\n    transform: rotate(-45deg) scaleX(-1);\r\n    animation: bubbleleft 0.4s ease 0.2s;\r\n    animation-fill-mode: forwards;\r\n    anim