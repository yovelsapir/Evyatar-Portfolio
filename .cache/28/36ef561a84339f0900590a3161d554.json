r\n\r\n.group {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-bottom: 200px;\r\n    margin-top: 150px;\r\n}\r\n\r\n.element {\r\n    position: relative;\r\n    transition: 0.5s all;\r\n}\r\n\r\n.hidden {\r\n    display: none;\r\n}\r\n\r\n.message-block {\r\n    position: relative;\r\n    transition: 0.5s all;\r\n    -webkit-transition: 0.5s all;\r\n    -moz-transition: 0.5s all;\r\n    -ms-transition: 0.5s all;\r\n    -o-transition: 0.5s all;\r\n}\r\n\r\n.message-block-last {\r\n    margin-bottom: 200px;\r\n}\r\n\r\n.right .message,\r\n.left .message {\r\n    position: relative;\r\n    transition: 1s opacity, all 0.4s;\r\n    -webkit-transition: 1s opacity, all 0.4s;\r\n    -moz-transition: 1s opacity, all 0.4s;\r\n    -ms-transition: 1s opacity, all 0.4s;\r\n    -o-transition: 1s opacity, all 0.4s;\r\n    opacity: 0;\r\n}\r\n\r\n@keyframes bubbleleft {\r\n    0% {\r\n        opacity: 0;\r\n        transform: rotate(-45deg) scaleX(-1);\r\n        left: -4.5px;\r\n    }\r\n\r\n    100% {\r\n        opacity: 1;\r\n        transform: rotate(0) scaleX(-1);\r\n        left: -16.5px;\r\n    }\r\n}\r\n\r\n@keyframes bubbleRight {\r\n    0% {\r\n        opacity: 0;\r\n        transform: rotate(45deg) scaleX(1);\r\n        right: -4.5px;\r\n    }\r\n\r\n    100% {\r\n        opacity: 1;\r\n        transform: rotate(0) scaleX(1);\r\n        right: -16.5px;\r\n    }\r\n}\r\n\r\n.left>svg {\r\n    position: absolute;\r\n    left: -16.5px;\r\n    bottom: -2px;\r\n    transform: rotate(-45deg) scaleX(-1);\r\n    animation: bubbleleft 0.4s ease 0.2s;\r\n    animation-fill-mode: forwards;\r\n    animation-delay: 0.4s;\r\n    fill: #e5e4e9;\r\n    opacity: 0;\r\n}\r\n\r\n.right>svg {\r\n    position: absolute;\r\n    right: -16.5px;\r\n    bottom: -2px;\r\n    transform: rotate(45deg) scaleX(1);\r\n    animation: bubbleRight 0.4s ease 0.2s;\r\n    animation-fill-mode: forwards;\r\n    animation-delay: 0.4s;\r\n    fill: #0253e2;\r\n    opacity: 0;\r\n}\r\n\r\n.message-block .show {\r\n    opacity: 1;\r\n}\r\n\r\n.right[data-emergence=hidden] .message {\r\n    left: 500px;\r\n}\r\n\r\n.right[data-emergence=visible] .message {\r\n    left: 0;\r\n    opacity: 1;\r\n}\r\n\r\n.left[data-emergence=hidden] .message {\r\n    left: -500px;\r\n}\r\n\r\n.left[data-emergence=visible] .message {\r\n    left: 0px;\r\n    opacity: 1;\r\n}\r\n\r\n#home-group .group {\r\n    margin-top: 50px;\r\n}\r\n\r\n#story-group .group {\r\n    margin-top: 200px;\r\n}\r\n\r\n.message-block {\r\n    display: block;\r\n    width: 960px;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.message {\r\n    display: inline-block;\r\n    padding: 10px 25px;\r\n    border-radius: 50px;\r\n    font-size: 26px;\r\n}\r\n\r\n.right .message {\r\n    background-color: #0253e2;\r\n    color: white;\r\n    float: right;\r\n}\r\n\r\n.left .message {\r\n    background-color: #e5e4e9;\r\n    color: rgb(59, 59, 59);\r\n}\r\n\r\n.gallery-block {\r\n    display: flex;\r\n    align-self: flex-start;\r\n    margin-top: 50px;\r\n    transition: 1s all;\r\n}\r\n\r\n.gallery-block .box {\r\n    position: relative;\r\n    background-color: #000000;\r\n    width: 400px;\r\n    height: 600px;\r\n    margin-right: 40px;\r\n    box-shadow: 0px 0px 20px 0px #00000055;\r\n}\r\n\r\n.gallery-block .box:hover img {\r\n    cursor: pointer;\r\n    opacity: 1;\r\n    transition: 0.5s all;\r\n}\r\n\r\n.box > img {\r\n    height: 100%;\r\n    width: 100%;\r\n    opacity: 0.3;\r\n}\r\n\r\n.gallery-block .thin-box {\r\n    width: 250px;\r\n    height: 400px;\r\n}\r\n\r\n.gallery-block-home-group {\r\n    position: relative;\r\n}\r\n\r\n.gallery-block-show-me-your-work {\r\n    position: relative;\r\n}\r\n\r\n.gallery-block-smooth-animation{\r\n    position: relative;\r\n}\r\n\r\n.button-group {\r\n    display: flex;\r\n    justify-content: center;\r\n    margin-top: 200px;\r\n}\r\n\r\n.btn {\r\n    margin: 0 5px;\r\n    background-color: transparent;\r\n    padding: 10px 20px;\r\n    border: 1px solid #dddddd;\r\n    transition: 0.3s all;\r\n    font-size: 20px;\r\n}\r\n\r\n.btn:hover {\r\n    background-color: #dddddd;\r\n    cursor: pointer;\r\n}\r\n\r\n.btn:focus {\r\n    border-color: #999999;\r\n    outline: none;\r\n}"},"lineCount":null}},"error":null,"hash":"b5329e8075d5063ecb567db960c14c72","cacheData":{"env":{}}}